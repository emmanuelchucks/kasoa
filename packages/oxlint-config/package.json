{
  "name": "@kasoa/oxlint-config",
  "version": "0.0.3",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./base": "./dist/base.json",
    "./node": "./dist/node.json",
    "./react": "./dist/react.json"
  },
  "scripts": {
    "dev": "node --watch src/build.ts",
    "build": "node src/build.ts",
    "lint": "oxlint --type-aware --fix .",
    "format": "oxfmt .",
    "typecheck": "tsgo --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^2.7.2",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint-plugin-import-lite": "^0.5.0",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.5",
    "eslint-plugin-unicorn-x": "^3.2.1",
    "eslint-plugin-unused-imports": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@typescript/native-preview": "7.0.0-dev.20260122.4",
    "ajv": "^8.17.1",
    "eslint-plugin-react-dom": "^2.7.2",
    "eslint-plugin-react-hooks-extra": "^2.7.2",
    "eslint-plugin-react-naming-convention": "^2.7.2",
    "eslint-plugin-react-web-api": "^2.7.2",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1"
  }
}
