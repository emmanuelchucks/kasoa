{
  "name": "@kasoa/oxlint-config",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./base": "./dist/base.json",
    "./node": "./dist/node.json",
    "./react": "./dist/react.json",
    "./plugins/sonarjs": "./dist/plugins/sonarjs.js",
    "./plugins/perfectionist": "./dist/plugins/perfectionist.js",
    "./plugins/react-compiler": "./dist/plugins/react-compiler.js",
    "./plugins/react-you-might-not-need-an-effect": "./dist/plugins/react-you-might-not-need-an-effect.js",
    "./plugins/eslint-react": "./dist/plugins/eslint-react.js",
    "./plugins/vitest": "./dist/plugins/vitest.js"
  },
  "scripts": {
    "dev": "tsgo --watch",
    "build": "tsgo && cp src/*.json dist/",
    "lint": "oxlint --type-aware --fix .",
    "format": "oxfmt .",
    "typecheck": "tsgo --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^2.7.2",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.5",
    "eslint-plugin-sonarjs": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@typescript/native-preview": "7.0.0-dev.20260120.1",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1"
  }
}
