    "typecheck": "tsgo --noEmit",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@kasoa/eslint-config": "workspace:*",
    "@kasoa/tsconfig": "workspace:*",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20251207.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "valibot": "^1.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=24"
